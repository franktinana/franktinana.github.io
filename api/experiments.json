permalink: /api/experiments.json
---

[
  {%- for e in site.experiments -%}
    {
      "title": {{ e.title | jsonify }},
      "summary": {{ e.summary | jsonify }},
      "url": {{ e.url | relative_url | jsonify }},
      "images": [
        {%- for img in e.images -%}
          {
            "src": {{ img.src | jsonify }},
            "alt": {{ img.alt | jsonify }}
          }{% unless forloop.last %},{% endunless %}
        {%- endfor -%}
      ]
    }{% unless forloop.last %},{% endunless %}
  {%- endfor -%}
]
JSON
