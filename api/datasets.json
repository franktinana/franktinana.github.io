---
layout: null
permalink: /api/datasets.json
---
[
{% for d in site.datasets %}
{
  "title": {{ d.title | jsonify }},
  "url": {{ d.url | relative_url | jsonify }},
  "summary": {{ (d.summary | default: d.excerpt | default: "") | strip | jsonify }},
  "links": {{ d.links | default: empty | jsonify }},
  "updated": {{ d.last_modified_at | default: d.date | default: "" | jsonify }}
}{% unless forloop.last %},{% endunless %}
{% endfor %}
]
